{{#each this}}
<div class="photo-card">
<img src="{{webformatURL}}" data-src="{{largeImageURL}}" alt="{{tags}}" loading="lazy" class="photo"/>
  <div class="info">
    <p class="info-item">
      <b>❤️  {{likes}}</b>
    </p>
    <p class="info-item">
      <b>👁  {{views}}</b>
    </p>
    <p class="info-item">
      <b>💬  {{comments}}</b>
    </p>
    <p class="info-item">
      <b>📩  {{downloads}}</b>
    </p>
  </div>
</div>
{{/each}}